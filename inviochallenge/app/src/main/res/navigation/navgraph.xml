<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.eliftekin.inviochallenge.ui.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_favoritesFragment2"
            app:destination="@id/favoritesFragment2" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragment2"
        android:name="com.eliftekin.inviochallenge.ui.fragments.FavoritesFragment"
        android:label="fragment_favorites"
        tools:layout="@layout/fragment_favorites" >
        <action
            android:id="@+id/action_favoritesFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/uniWebFragment2"
        android:name="com.eliftekin.inviochallenge.ui.fragments.UniWebFragment"
        android:label="fragment_uni_web"
        tools:layout="@layout/fragment_uni_web" >
        <action
            android:id="@+id/action_uniWebFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
        <action
            android:id="@+id/action_uniWebFragment2_to_favoritesFragment2"
            app:destination="@id/favoritesFragment2" />
    </fragment>
</navigation>